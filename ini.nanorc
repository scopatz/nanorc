syntax "INI" "\.(ini|desktop|lfl|override)$" "(mimeapps\.list|pinforc|setup\.cfg)$" "weechat/.+\.conf$"
header "^\[[A-Za-z]+\]$"

color cyan "\<(true|false)\>"
color cyan "^[[:space:]]*[^=]*="
color magenta "^[[:space:]]*\[.*\]$"
color red "[=;]"
color yellow ""(\\.|[^"])*"|'(\\.|[^'])*'"
color black "(^|[[:space:]])(#([^{].*)?|;.*)$"
color ,green "[[:space:]]+$"
color ,red "	+ +| +	+"
